<div class="graphFunctionInteraction">
    <div class="prompt">
        {{{prompt}}}
    </div>
    <div class="shape-panel">
        <div class="shape-container"></div>
        <div class="shape-controls">
            <button value="linear" class="btn btn-info" type="button">Linear</button>
            <button value="absolute" class="btn btn-info" type="button">Absolute Value</button>
            <button value="quadratic" class="btn btn-info" type="button">Quadratic</button>
            <button value="exponential" class="btn btn-info" type="button">Exponential</button>
            <button value="logarithmic" class="btn btn-info" type="button">Logarithmic</button>
            <button value="cosine" class="btn btn-info" type="button">Sin/Cos</button>
            <button value="tan" class="btn btn-info" type="button">Tan/Cotan</button>
        </div>
    </div>
</div>