<?xml version="1.0" encoding="UTF-8"?><assessmentItem xmlns="http://www.imsglobal.org/xsd/apip/apipv1p0/qtiitem/imsqti_v2p1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:pci="http://www.imsglobal.org/xsd/portableCustomInteraction" adaptive="false" identifier="VF643134" timeDependent="false" title="Composite" toolName="IBIS Export" toolVersion="1.0" xsi:schemaLocation="http://www.imsglobal.org/xsd/apip/apipv1p0/qtiitem/imsqti_v2p1 http://www.imsglobal.org/profile/apip/apipv1p0/apipv1p0_qtiitemv2p1_v1p0.xsd http://www.imsglobal.org/xsd/apip/apipv1p0/imsapip_qtiv1p0 http://www.imsglobal.org/profile/apip/apipv1p0/apipv1p0_qtiextv2p1_v1p0.xsd">
    <responseDeclaration baseType="string" cardinality="multiple" identifier="RESPONSE">
        <correctResponse>
            <value>50 25</value>
            <value>100 50</value>
        </correctResponse>
    </responseDeclaration>
    <outcomeDeclaration baseType="float" cardinality="single" identifier="SCORE" normalMaximum="2" normalMinimum="0">
        <defaultValue>
            <value>0</value>
        </defaultValue>
    </outcomeDeclaration>
    <templateDeclaration baseType="string" cardinality="single" identifier="EQUATION">
        <defaultValue>
            <value>y=x/2</value>
        </defaultValue>
    </templateDeclaration>
    <templateDeclaration baseType="integer" cardinality="single" identifier="NumberofPointsRequired">
        <defaultValue>
            <value>2</value>
        </defaultValue>
    </templateDeclaration>
    <itemBody>
        <customInteraction responseIdentifier="RESPONSE">
            <pci:portableCustomInteraction scustomInteractionTypeIdentifier="graphPointLineGraphInteraction" hook="graphPointLineGraphInteraction/runtime/graphPointLineGraphInteraction.js">
                <pci:responseSchema href="http://imsglobal.org/schema/json/v1.0/response.json"/>
                <pci:resources location="http://imsglobal.org/pci/1.0.15/sharedLibraries.xml">
                    <pci:libraries>
                        <pci:lib id="IMSGlobal/jquery_2_1_1"/>
                        <pci:lib id="PARCC/graphFunction"/>
                        <pci:lib id="PARCC/gridFactory"/>
                        <pci:lib id="PARCC/pointFactory"/>
                        <pci:lib id="PARCC/lineFactory"/>
                    </pci:libraries>
                </pci:resources>
                <pci:properties>
                    <pci:entry key="draggable">true</pci:entry>
                    <pci:entry key="graphTitle">Culture 2 Data</pci:entry>
                    <pci:entry key="graphTitleRequired">true</pci:entry>
                    <pci:entry key="graphType">line</pci:entry>
                    <pci:entry key="maxPoints">6</pci:entry>
                    <pci:entry key="plotColor">#0000FF</pci:entry>
                    <pci:entry key="plotThickness">6</pci:entry>
                    <pci:entry key="pointColor">#0000FF</pci:entry>
                    <pci:entry key="pointGlow">true</pci:entry>
                    <pci:entry key="pointRadius">8</pci:entry>
                    <pci:entry key="segment">true</pci:entry>
                    <pci:entry key="weight">1</pci:entry>
                    <pci:entry key="xAllowOuter">true</pci:entry>
                    <pci:entry key="xStep">1</pci:entry>
                    <pci:entry key="xLabel">Time (hr)</pci:entry>
                    <pci:entry key="xEnd">7</pci:entry>
                    <pci:entry key="xStart">0</pci:entry>
                    <pci:entry key="xSubStep">2</pci:entry>
                    <pci:entry key="xUnit">42</pci:entry>
                    <pci:entry key="xWeight">3</pci:entry>
                    <pci:entry key="yAllowOuter">true</pci:entry>
                    <pci:entry key="yStep">10</pci:entry>
                    <pci:entry key="yLabel">Number of Cells (x1,000,000)</pci:entry>
                    <pci:entry key="yEnd">100</pci:entry>
                    <pci:entry key="yStart">0</pci:entry>
                    <pci:entry key="ySubStep">2</pci:entry>
                    <pci:entry key="yUnit">3.4</pci:entry>
                    <pci:entry key="yWeight">3</pci:entry>
                </pci:properties>
                <pci:markup>
                    <html5:div class="graphPointLineGraphInteraction">
                        <html5:div class="prompt"></html5:div>
                        <html5:div class="shape-panel">
                            <html5:div class="shape-container"></html5:div>
                        </html5:div>
                    </html5:div>
                </pci:markup>
            </pci:portableCustomInteraction>
        </customInteraction>
    </itemBody>
    <responseProcessing>
        <responseCondition>
            <responseIf>
                <gte>
                    <customOperator class="qti.customOperators.math.graph.CountPointsThatSatisfyEquation">
                        <variable identifier="RESPONSE"/>
                        <variable identifier="EQUATION"/>
                    </customOperator>
                    <default identifier="NumberofPointsRequired"/>
                </gte>
                <setOutcomeValue identifier="SCORE">
                    <sum>
                        <variable identifier="SCORE"/>
                        <baseValue baseType="float">1</baseValue>
                    </sum>
                </setOutcomeValue>
            </responseIf>
        </responseCondition>
    </responseProcessing>
    <apip:apipAccessibility xmlns:apip="http://www.imsglobal.org/xsd/apip/apipv1p0/imsapip_qtiv1p0"/>
</assessmentItem>
